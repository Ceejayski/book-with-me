<section id="rentalListing">
  <h1 class="title">Your Home All Around the World</h1>
  <div class="row">
    <div class='col-md-3 col-xs-6'>
      <div class="card bwm-card">
        <img class="card-img-top" src="{{model.image}}" alt="">
        <div class="card-block">
          <h6 class="card-subtitle">{{model.category}} &#183; {{model.city}}</h6>
          <h4 class="card-title">{{model.title}}</h4>
          <p class="card-text">${{model.daily_rate}} per Night &#183; Free Cancelation</p>
          <a href="#" class="card-link">More Info</a>
        </div>
      </div>
    </div>
  </div>
</section>
